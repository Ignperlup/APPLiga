<?xml version='1.0' encoding='utf-8'?>
<widget id="org.devgirl.framework7.mediaexplorer" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Liga de Padel Asociación Parque Flores</name>
    <description>
        4ª Liga de Padel
    </description>
    <author email="pl.ignacio@gmail.com" href="http://cordova.io">
        Ignacio Perez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    

   
    <icon src="icon.png" />

    <icon src="res/icon/android/icon-48-mdpi.png"   platform="android"    qualifier="mdpi" />
    <icon src="res/icon/android/icon-72-hdpi.png"   platform="android"    qualifier="hdpi" />
    <icon src="res/icon/android/icon-96-xhdpi.png"  platform="android"    qualifier="xhdpi" />
    <icon src="res/icon/android/icon-96-xxhdpi.png"  platform="android"    qualifier="xxhdpi" />
    <icon src="res/icon/android/icon-96-xxxhdpi.png"  platform="android"    qualifier="xxxhdpi" />

    <splash src="res/screen/android/splash-port-hdpi.png" platform="android" density="port-hdpi"/>
    <splash src="res/screen/android/splash-port-ldpi.png" platform="android" density="port-ldpi"/>
    <splash src="res/screen/android/splash-port-mdpi.png" platform="android" density="port-mdpi"/>
    <splash src="res/screen/android/splash-port-xhdpi.png" platform="android" density="port-xhdpi"/>
    <splash src="res/screen/android/splash-port-xxhdpi.png" platform="android" density="port-xxhdpi"/>
    <splash src="res/screen/android/splash-port-xxxhdpi.png" platform="android" density="port-xxxhdpi"/>



    <platform name="android">
     

    </platform>
   
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
   
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="3000" />



    <plugin name="cordova-plugin-console" spec="1.0.7" />
    <plugin name="cordova-plugin-splashscreen" spec="4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <plugin name="pushbots-cordova-plugin" spec="1.4.7" />
</widget>
